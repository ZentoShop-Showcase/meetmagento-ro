---
import Layout from '../../layouts/Layout.astro';
import PageBanner from '../../components/PageBanner.astro';
import SubNavigation from '../../components/Past/SubNavigation.astro';
import Talk from '../../components/Past/Talk.astro';
import Newsletter from '../../components/Newsletter.astro';

import ImgThomasGoletz from '../../assets/talks/mm16ro/thomasgoletz.png';
import ImgJasonWoosley from '../../assets/talks/mm16ro/jasonwoosley.png';

import ImgPanel1 from '../../assets/talks/mm16ro/panel1.png';
import ImgCiprianPasca from '../../assets/talks/mm16ro/ciprianpasca.png';
import ImgIulianPadurariu from '../../assets/talks/mm16ro/iulianpadurariu.png';
import ImgDeryckHarlick from '../../assets/talks/mm16ro/deryckharlick.png';
import ImgCalinPopa from '../../assets/talks/mm16ro/calinpopa.png';
import ImgPaulBrie from '../../assets/talks/mm16ro/paulbrie.png';
import ImgLauraVoica from '../../assets/talks/mm16ro/lauravoica.png';
import ImgKubaZwolinski from '../../assets/talks/mm16ro/kubazwolinski.png';
import ImgSergeyLysak from '../../assets/talks/mm16ro/sergeylysak.png';

import ImgBenMarks from '../../assets/talks/mm16ro/benmarks.png';
import ImgMariusStrajeru from '../../assets/talks/mm16ro/mariusstrajeru.png';
import ImgFabianBlechschmidt from '../../assets/talks/mm16ro/fabianblechschmidt.png';
import ImgDavidManners from '../../assets/talks/mm16ro/davidmanners.png';
import ImgAntonKaplya from '../../assets/talks/mm16ro/antonkaplya.png';
import ImgAnnaVolkl from '../../assets/talks/mm16ro/annavolkl.png';
import ImgVinaiKopp from '../../assets/talks/mm16ro/vinaikopp.png';
import ImgVolodymyrKublytskyi from '../../assets/talks/mm16ro/volodymyrkublytskyi.png';
import ImgMiguelBalparda from '../../assets/talks/mm16ro/miguelbalparda.png';
import ImgVladTirla from '../../assets/talks/mm16ro/vladtirla.png';
import ImgSonjaRiesterer from '../../assets/talks/mm16ro/sonjariesterer.png';
import ImgPanel2 from '../../assets/talks/mm16ro/panel2.png';
import ImgAndraLungu from '../../assets/talks/mm16ro/andralungu.png';
import ImgFabiannSchmengler from '../../assets/talks/mm16ro/fabianschmengler.png';
import ImgPavloOkhrem from '../../assets/talks/mm16ro/pavlookhrem.png';
import ImgOptaros from '../../assets/talks/mm16ro/optaros.png';

---

<Layout title="Meet Magento 2016">
	<main>
		<PageBanner title="Meet Magento 2016" />
        <SubNavigation selected="mm16ro" />

		<div class="container flex items-center flex-wrap mx-auto pb-20 sm:pb-26 md:flex-nowrap md:gap-6">
			<div class="w-full sm:w-1/2 flex-col pt-4 sm:order-first order-last">
				<h5>2016 Edition</h5>
				<h2 class="pb-3">Photos</h2>
				<p class="text-l font-normal text-gray-600 pb-3">Meet Magento Romania 2016 was held on October 28-29 at Grand Hotel Napoca, Cluj-Napoca.</p>
				<p class="text-l font-normal text-gray-600 pb-8">Check out the photo albums on Facebook and tag yourself.</p>
				<div class="flex w-full">
					<a
						id="photos-fb-link"
						class="flex-1 w-full max-w-48 bg-orange-600 button-m text-white rounded
							py-4 px-4 text-center hover:bg-orange-700 focus:outline-none"
						href="https://www.facebook.com/media/set/?set=a.1152905254744669&type=3"
						target="_blank"
					>
						Day 1
					</a>
					<a
						id="photos-fb-link"
						class="flex-1 w-full max-w-48 bg-orange-600 button-m text-white rounded
							py-4 px-4 mx-4 text-center hover:bg-orange-700 focus:outline-none"
						href="https://www.facebook.com/media/set/?set=a.1152919038076624&type=3"
						target="_blank"
					>
						Day 2
					</a>
					<a
						id="photos-fb-link"
						class="flex-1 w-full max-w-48 bg-orange-600 button-m text-white rounded
							py-4 px-4 mx-4 text-center hover:bg-orange-700 focus:outline-none"
						href="https://www.facebook.com/media/set/?set=a.1152924568076071&type=3"
						target="_blank"
					>
						Party
					</a>
				</div>
			</div>
			<div class="w-full sm:w-1/2 flex-col pt-4">
			</div>
		</div>

        <h2 class="container pb-4 sm:pb-8">Presentations</h2>
        <h4 class="container pb-4">Keynote</h4>
        <div class="container grid grid-cols-1 md:grid-cols-2 mx-auto pb-4 sm:pb-4 gap-6">
            <Talk 
                image={ImgThomasGoletz}
                title="Quo vadis, Meet Magento? Where do we go?" 
                language="en"
                speaker="Thomas Goletz" 
                speakerCompany="Meet Magento Association" 
            />
            <Talk 
                image={ImgJasonWoosley}
                title="Open Source Community Engagement 2.0 - Rediscovering distributed contributions" 
                language="en"
                speaker="Jason Woosley" 
                speakerCompany="Magento" 
            />
        </div>
        <h4 class="container py-4">Business Track</h4>
        <div class="container grid grid-cols-1 md:grid-cols-2 mx-auto pb-4 sm:pb-4 gap-6">
            <Talk
                image={ImgPanel1}
                title="eCommerce Business Panel" 
                language="ro"
                speaker="Evozon, BT, Google, FanCurier" 
                speakerCompany=""
            />
            <Talk
                image={ImgCiprianPasca}
                title="IT ecosystem Cluj - case-studies" 
                language="ro"
                speaker="Ciprian Pașca" 
                speakerCompany="Sprint Point"
            />
            <Talk
                image={ImgIulianPadurariu}
                title="The Sharing Economy" 
                language="en"
                speaker="Iulian Padurariu" 
                speakerCompany="GetPony"
            />
            <Talk
                image={ImgDeryckHarlick}
                title="Flash points for Merchants and Agencies that can kill a beautiful friendship!" 
                language="en"
                speaker="Deryck Harlick" 
                speakerCompany=""
            />
            <Talk
                image={ImgCalinPopa}
                title="The ABC of cloud and cloud solutions" 
                language="en"
                speaker="Călin Popa" 
                speakerCompany="Microsoft"
            />
            <Talk
                image={ImgPaulBrie}
                title="Mobile usage is increasing worldwide: why should you care?" 
                language="ro"
                speaker="Paul Brie" 
                speakerCompany="Evozon"
            />
            <Talk
                image={ImgLauraVoica}
                title="Thinking ahead and meeting brand objectives with Magento" 
                language="ro"
                speaker="Laura Voica" 
                speakerCompany="Arctic"
            />
            <Talk
                image={ImgKubaZwolinski}
                title="Internet of Things in Omnichannel Commerce with Magento" 
                language="en"
                speaker="Kuba Zwolinski" 
                speakerCompany="Snowdog"
            />
            <Talk
                image={ImgSergeyLysak}
                title="Magento 2 SEO Lucky Chance" 
                language="en"
                speaker="Sergey Lysak" 
                speakerCompany="Eltrino"
            />
        </div>
        <h4 class="container py-4">Technical Track</h4>
        <div class="container grid grid-cols-1 md:grid-cols-2 mx-auto pb-4 sm:pb-4 gap-6">
            <Talk 
                image={ImgBenMarks}
                title="Moving Magento 2 Forward" 
                language="en"
                speaker="Ben Marks" 
                speakerCompany="Magento" 
            />
            <Talk 
                image={ImgMariusStrajeru}
                title="Best Worst Magento Extension" 
                language="en"
                speaker="Marius Străjeru" 
                speakerCompany="Magento Master" 
            />
            <Talk 
                image={ImgFabianBlechschmidt}
                title="Fucking up projects - a manual" 
                language="en"
                speaker="Fabian Blechschmidt" 
                speakerCompany="" 
            />
            <Talk 
                image={ImgDavidManners}
                title="Don't be a git" 
                language="en"
                speaker="David Manners" 
                speakerCompany="Magento Master" 
            />
            <Talk 
                image={ImgAntonKaplya}
                title="Staging tech" 
                language="en"
                speaker="Anton Kaplya" 
                speakerCompany="Magento" 
            />
            <Talk 
                image={ImgAnnaVolkl}
                title="Secure input and output handling" 
                language="en"
                speaker="Anna Völkl" 
                speakerCompany="Limesoda, Magento Master" 
            />
            <Talk 
                image={ImgVinaiKopp}
                title="Writing testable code for Magento 1 &amp; 2" 
                language="en"
                speaker="Vinai Kopp" 
                speakerCompany="Magento Master" 
                video="https://youtu.be/jHYc0qw7Ih0"
            />
            <Talk 
                image={ImgVolodymyrKublytskyi}
                title="Magento Presentation Layer" 
                language="en"
                speaker="Volodymyr Kublytskyi" 
                speakerCompany="Magento" 
            />
            <Talk 
                image={ImgMiguelBalparda}
                title="Magento 2: Premium Performance with PHP 7 and Varnish" 
                language="en"
                speaker="Miguel Balparda" 
                speakerCompany="Nexcess, Magento Master" 
            />
            <Talk 
                image={ImgVladTirla}
                title="Creating a delightful customer experience through micro-moments" 
                language="en"
                speaker="Vlad Tîrlă" 
                speakerCompany="Evozon" 
            />
            <Talk 
                image={ImgSonjaRiesterer}
                title="Seek and ye shall find - Product Search in Magento 2" 
                language="en"
                speaker="Sonja Riesterer" 
                speakerCompany="" 
            />
            <Talk 
                image={ImgPanel2}
                title="Magento Masters Panel" 
                language="en"
                speaker="Ben, Vinai, Marius, Anna, Kuba, Miguel, Sander, David" 
                speakerCompany="" 
            />
            <Talk 
                image={ImgAndraLungu}
                title="Magento 2 Hands-on" 
                language="en"
                speaker="Andra Lungu" 
                speakerCompany="Bitbull" 
            />
            <Talk 
                image={ImgFabiannSchmengler}
                title="Porting A Complex Extension To Magento 2" 
                language="en"
                speaker="Fabian Schmengler" 
                speakerCompany="" 
            />
            <Talk 
                image={ImgPavloOkhrem}
                title="12 Ways to improve Magento 2 performance and security" 
                language="en"
                speaker="Pavlo Okhrem" 
                speakerCompany="Elogic Commerce" 
            />
            <Talk 
                image={ImgOptaros}
                title="How NOT to sell online " 
                language="en"
                speaker="Andrei Tătaru" 
                speakerCompany="Optaros" 
            />
        </div>

        <div class="container flex items-center flex-wrap mx-auto pt-12 pb-20 sm:pt-18 sm:pb-26 md:flex-nowrap md:gap-6">
            <div class="w-full flex-col order-last pt-4 sm:w-1/2 sm:pt-0 sm:order-first">
                <h5>Organised by</h5>
                <h2 class="pb-3">MindMagnet</h2>
            </div>
            <div class="flex w-full h-48 bg-purple-50 flex-col pt-4 sm:w-1/2 sm:pt-0 items-center justify-center">
                <img src="/assets/sponsors/mindmagnet.svg" alt="MindMagnet" class="w-1/2 mx-auto" />
            </div>
        </div>

		<Newsletter />
	</main>
</Layout>
