                video="https://youtu.be/eGbglrcwjB0"
            />
            <Talk 
                image='/assets/talks/mm17ro/razvanavramescu.jpg' 
                title="Magento 2 on Kubernetes" 
                language="en"
                speaker="Răzvan Avramescu" 
                speakerCompany="" 
                video="https://youtu.be/mERet8xUoA0"
