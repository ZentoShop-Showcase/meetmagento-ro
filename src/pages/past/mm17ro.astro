---
import Layout from '../../layouts/Layout.astro';
import PageBanner from '../../components/PageBanner.astro';
import SubNavigation from '../../components/Past/SubNavigation.astro';
import Talk from '../../components/Past/Talk.astro';
import Newsletter from '../../components/Newsletter.astro';
---

<Layout title="Meet Magento 2017">
	<main>
		<PageBanner title="Meet Magento 2017" />
        <SubNavigation selected="mm17ro" />

        <h2 class="container pb-4 sm:pb-8">Presentations</h2>
        <h4 class="container pb-4">Keynote</h4>
        <div class="container grid grid-cols-1 md:grid-cols-2 mx-auto pb-4 sm:pb-4 gap-6">
            <Talk 
                image='/assets/talks/mm17ro/benmarks.png' 
                title="The future of Magento" 
                language="en"
                speaker="Ben Marks" 
                speakerCompany="Magento" 
                video="https://youtu.be/LlLWpaqGOL8"
            />
        </div>
        <h4 class="container py-4">Business Track</h4>
        <div class="container grid grid-cols-1 md:grid-cols-2 mx-auto pb-4 sm:pb-4 gap-6">
            <Talk
                image='/assets/talks/mm17ro/elenadobre.png' 
                title="Promovarea pe mobil - aplicații și mobi-site-uri - 7 recomandări de performanță" 
                language="ro"
                speaker="Elena Dobre" 
                speakerCompany="Zarbi"
                video="https://youtu.be/oXmotz4FRD8"
            />
            <Talk 
                image='/assets/talks/mm17ro/bogdanherea.jpg' 
                title="Roundtable - Provocarile unui eCommerce startup în 2018" 
                language="ro"
                speaker="Bogdan Herea" 
                speakerCompany="PitechPlus" 
            />
            <Talk 
                image='/assets/talks/mm17ro/dorinboerescu.png' 
                title="Performance marketing in sharing economy" 
                language="ro"
                speaker="Dorin Boerescu" 
                speakerCompany="2Performant" 
                video="https://youtu.be/uEaT7xWn_LU"
            />
            <Talk 
                image='/assets/talks/mm17ro/babasan.jpg' 
                title="GoOnline Roundtable" 
                language="ro"
                speaker="Sergiu Babașan, Ovidiu Kalman, Marius Panait, Marius Flore" 
                speakerCompany="Optimized" 
                video="https://youtu.be/d74s9856XE8"
            />
            <Talk 
                image='/assets/talks/mm17ro/robertberza.jpg' 
                title="BlackFriday la FashionDays - interviu" 
                language="ro"
                speaker="Robert Berza" 
                speakerCompany="FashionDays" 
            />
            <Talk 
                image='/assets/talks/mm17ro/valentinradu.jpg' 
                title="How and why to make your Magento eCommerce Customer-Centric" 
                language="ro"
                speaker="Valentin Radu" 
                speakerCompany="Omniconvert" 
                video="https://youtu.be/mmpLJgX_wm0"
            />
            <Talk 
                image='/assets/talks/mm17ro/bogdancolceriu.jpg' 
                title="The actual costs for operating an online store" 
                language="ro"
                speaker="Bogdan Colceriu" 
                speakerCompany="Frisbo" 
                video="https://youtu.be/g2gIGTTwJeI"
            />
            <Talk 
                image='/assets/talks/mm17ro/adrianilovan.jpg' 
                title="Magento 2 vs WooCommerce pentru magazine online B2C și B2B" 
                language="ro"
                speaker="Adrian Ilovan" 
                speakerCompany="Netlogic" 
                video="https://youtu.be/55w6hgram64"
            />
            <Talk 
                image='/assets/talks/mm17ro/alexandracohutiu.jpg' 
                title="From Setting Up The Relationship With The Investors to Data Protection" 
                language="ro"
                speaker="Alexandra Cohutiu" 
                speakerCompany="LLA" 
                video="https://youtu.be/DRcec6AnHAE"
            />

            <Talk 
                image='/assets/talks/mm17ro/guidojansen.jpg' 
                title="Global and local strategies for Merchants and Agencies" 
                language="en"
                speaker="Guido Jansen" 
                speakerCompany="" 
                video="https://youtu.be/UEuyMR2c1-c"
            />
            <Talk 
                image='/assets/talks/mm17ro/cristianfilimon.jpg' 
                title="Cum sa iti optimizezi relatia cu agentia de webdevelopment" 
                language="ro"
                speaker="Cristian Filimon" 
                speakerCompany="Symbolic" 
                video="https://youtu.be/Ijrd8xhz3Xo"
            />
        </div>
        <h4 class="container py-4">Technical Track</h4>
        <div class="container grid grid-cols-1 md:grid-cols-2 mx-auto pb-4 sm:pb-4 gap-6">
            <Talk 
                image='/assets/talks/mm17ro/mariusstrajeru.jpg' 
                title="Because it's in the core" 
                language="en"
                speaker="Marius Străjeru" 
